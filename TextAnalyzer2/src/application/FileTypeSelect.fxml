<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.FileTypeSelectController">
   <children>
      <VBox prefHeight="113.0" prefWidth="213.0" spacing="20.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label text="Select a parser for this file:" />
            <ComboBox fx:id="fileTypeSelector" prefWidth="150.0" />
            <VBox alignment="CENTER_RIGHT" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="confirmFileTypeButton" alignment="CENTER_RIGHT" mnemonicParsing="false" onAction="#fileTypeSelected" text="Next" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
